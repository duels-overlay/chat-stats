{
  "name": "chat-stats",
  "version": "1.0.0-prerelease1",
  "description": "Duels Overlay without the overlay",
  "main": "dist/index.js",
  "scripts": {
    "test": "node .",
    "watch": "tsc -w",
    "package": "npx pkg package.json --targets node14-win-x64"
  },
  "bin": "./dist/index.js",
  "plugins": [
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/duels-overlay/chat-stats.git"
  },
  "pkg": {
    "scripts": "dist/**/*.js",
    "targets": [
      "node14-win-x64"
    ],
    "options": [
      "experimental-modules"
    ],
    "outputPath": "package"
  },
  "author": "zani",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/duels-overlay/chat-stats/issues"
  },
  "homepage": "https://github.com/duels-overlay/chat-stats#readme",
  "dependencies": {
    "colors": "^1.4.0",
    "hypixel-proxy": "^1.0.6"
  },
  "devDependencies": {
    "@types/node": "^16.3.2",
    "@types/node-fetch": "^2.5.11",
    "pkg": "^5.3.0",
    "typescript": "^4.3.5"
  }
}
